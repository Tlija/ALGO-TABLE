ALGORITHM produit_scalaire
VAR
   Tab V(3),Tab U(3): ARRAY_OF INTEGER;
   i:INTEGER;
   P:INTEGER:=0;
BEGIN
   FOR i FROM 0 TO max 1 step  DO
    Write('dommer l element ',i+1, "du vecteur V");
    Write(V(i));
     Write('dommer l element ',i+1, "du vecteur U");
    Write(U(i));
   END_FOR
   FOR i FROM 0 TO max 1 step  DO
    p:=p+V(i)*U(i);
   END_FOR
  Write("le produit scalaire des vecteur u et v :",P)